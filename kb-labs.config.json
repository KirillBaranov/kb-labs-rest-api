{
  "sync": {
    "enabled": true
  },
  "rest": {
    "port": 3001,
    "basePath": "/api/v1",
    "auth": {
      "mode": "none"
    },
    "queue": {
      "driver": "memory"
    },
    "cli": {
      "bin": "pnpm",
      "prefix": ["kb"],
      "timeoutSec": 900
    },
    "storage": {
      "driver": "fs",
      "baseDir": ".kb/rest"
    },
    "plugins": [],
    "mockMode": false,
    "cors": {
      "origins": ["http://localhost:3000"],
      "allowCredentials": true
    }
  }
}