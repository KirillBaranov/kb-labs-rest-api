{
  "name": "@kb-labs/rest-api-contracts",
  "version": "1.0.0",
  "description": "Shared API contracts for KB Labs REST/CLI/Studio surfaces",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "test": "vitest run --passWithNoTests",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/studio-contracts": "link:../../../kb-labs-studio/packages/studio-contracts",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
